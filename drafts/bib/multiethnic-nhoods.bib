@article{bader_community_2015,
	title = {Community attraction and avoidance in {Chicago}: {What}'s race got to do with it?},
	volume = {660},
	url = {http://ann.sagepub.com/content/660/1/261.abstract},
	doi = {10.1177/0002716215577615},
	abstract = {Existing theories of racial residential segregation predicted more dramatic declines in segregation than we have seen in the past two decades. We argue that the failure to find the predicted declines is the result of failing to study the processes of residential search and the perceptions upon which those searches are based. These perceptions are a critical but often-ignored component of the residential sorting process that ultimately translates into persistent segregation, but also offers hope for future integration.  We examine where Chicago-area residents would “seriously consider” and “never consider” living and find that community attraction and avoidance are highly racialized. Additionally, we find that respondents would avoid a much higher number of communities, suggesting people attempt to limit choice sets when considering where they would move. Race is most influential for the perceptions of whites and least for that of blacks. Latinos perceptions are substantially influenced by correlated demographic characteristics.},
	number = {1},
	journal = {The ANNALS of the American Academy of Political and Social Science},
	author = {Bader, Michael D. M. and Krysan, Maria},
	month = jul,
	year = {2015},
	pages = {261--281},
