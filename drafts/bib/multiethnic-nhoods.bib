	author = {Coulter, Rory and Scott, Jacqueline},
	year = {2015},
	keywords = {migration, life course, motivation, reason, residential mobility},
	pages = {354--371},
	file = {Full Text PDF:/Users/bader/Zotero/storage/SIIEH7J5/Coulter and Scott - 2015 - What Motivates Residential Mobility Re-examining .pdf:application/pdf;Snapshot:/Users/bader/Zotero/storage/LXF428F5/psp.html:text/html}
}

