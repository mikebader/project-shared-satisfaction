	author = {Hwang, Jackelyn},
	year = {2016},
	keywords = {Demography, Gentrification, Medicine/Public Health, general, Population Economics, Race and Ethnicity, Sociology, general, immigration, Geography, general, Multiethnic neighborhoods, Enclave},
	pages = {189--213},
	file = {Full Text PDF:/Users/bader/Zotero/storage/NJWG5IC5/Hwang - 2015 - Pioneers of Gentrification Transformation in Glob.pdf:application/pdf;Snapshot:/Users/bader/Zotero/storage/DBBP84X8/s13524-015-0448-4.html:text/html}
}

